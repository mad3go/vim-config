    "E:\\Keil_Project\\STM32F401RET6_neo智能锁ver1\\CMSIS",
    "E:\\Keil_Project\\STM32F401RET6_neo智能锁ver1\\CMSIS\\Include",
    "E:\\Keil_Project\\STM32F401RET6_neo智能锁ver1\\HARDWARE\\inc",
    "E:\\Keil_Project\\STM32F401RET6_neo智能锁ver1\\USER\\inc"
  ],
  "C_Cpp.default.intelliSenseMode": "gcc-x64",
  "terminal.external.windowsExec": "C:\\Windows\\System32\\cmd.exe",
  "terminal.integrated.defaultProfile.windows": "PowerShell",
